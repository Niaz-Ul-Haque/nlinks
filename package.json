{
   "name": "nlinks",
   "version": "1.0.0",
   "description": "A CLI link checker that checks out if a link is working or not. Links can be plucked out from a filee or direct.",
   "main": "src/index.js",
   "bin": {
      "@Niaz/nlinks": "bin/nlinks",
      "nlinks": "bin/nlinks"
   },
   "publishConfig": {
      "access": "public"
   },
   "scripts": {
      "prettier": "prettier --write \"./**/*.{md,jsx,json,html,css,js,yml}\"",
      "prettier-check": "prettier --check \"./**/*.{md,jsx,json,html,css,js,yml}\"",
      "eslint-fix": "eslint --fix --ignore-path .gitignore .",
      "eslint": "eslint --ignore-path .gitignore .",
      "test": "jest"
   },
   "repository": {
      "type": "git",
      "url": "git+https://github.com/Niaz-Ul-Haque/nlinks.git"
   },
   "keywords": [
      "cli",
      "nlinks"
   ],
   "author": "Mohammed Niaz Ul Haque",
   "license": "ISC",
   "bugs": {
      "url": "https://github.com/Niaz-Ul-Haque/nlinks/issues"
   },
   "homepage": "https://github.com/Niaz-Ul-Haque/nlinks#readme",
   "dependencies": {
      "arg": "^4.1.3",
      "chalk": "^4.1.0",
      "dotenv": "^8.2.0",
      "esm": "^3.2.25",
      "inquirer": "^7.3.3",
      "linkinator": "^2.1.2",
      "node-fetch": "^2.6.1"
   },
   "devDependencies": {
      "eslint": "^7.14.0",
      "jest": "^26.6.3",
      "prettier": "2.1.2"
   }
}
